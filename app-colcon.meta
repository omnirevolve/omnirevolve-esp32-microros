{
    "names": {
        "rmw_microxrcedds": {
            "cmake-args": [
                "-DRMW_UXRCE_MAX_NODES=1",
                "-DRMW_UXRCE_MAX_PUBLISHERS=2",
                "-DRMW_UXRCE_MAX_SUBSCRIPTIONS=4",
                "-DRMW_UXRCE_MAX_SERVICES=0",
                "-DRMW_UXRCE_MAX_CLIENTS=0",
                "-DRMW_UXRCE_MAX_HISTORY=4",
                "-DRMW_UXRCE_TRANSPORT=udp",
                "-DRMW_UXRCE_STREAM_HISTORY=8",
                "-DRMW_UXRCE_MAX_INPUT_BUFFER_SIZE=1024",
                "-DRMW_UXRCE_MAX_OUTPUT_BUFFER_SIZE=1024"
            ]
        },
        "microxrcedds_client": {
            "cmake-args": [
                "-DUCLIENT_UDP_TRANSPORT_MTU=1024",
                "-DUCLIENT_TCP_TRANSPORT_MTU=1024", 
                "-DUCLIENT_SERIAL_TRANSPORT_MTU=1024",
                "-DUCLIENT_MAX_OUTPUT_BEST_EFFORT_STREAMS=4",
                "-DUCLIENT_MAX_INPUT_BEST_EFFORT_STREAMS=4",
                "-DUCLIENT_MAX_OUTPUT_RELIABLE_STREAMS=4",
                "-DUCLIENT_MAX_INPUT_RELIABLE_STREAMS=4",
                "-DUCLIENT_STREAM_HISTORY=8",
                "-DUCLIENT_STREAM_BUFFER_SIZE=1024"
            ]
        }
    }
}